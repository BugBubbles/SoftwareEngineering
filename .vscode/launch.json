{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Node.js调试器",
      "program": "${file}",
      "stopOnEntry": false,
      "args": [],
      "cwd": "${workspaceRoot}/Jscript",
      "runtimeExecutable": null,
      "console": "integratedTerminal",
      "runtimeArgs": [
        "--nolazy"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "java",
      "name": "Java调试器",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "name": "Python调试器",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "internalConsole",
      "justMyCode": false
    },
    {
      "name": "C/C++调试器",
      "type": "cppdbg",
      "request": "launch",
      "targetArchitecture": "x64",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "miDebuggerPath": "F:/Program Files/mingw64/bin/gdb.exe",
      "MIMode": "gdb",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "externalConsole": false,
      "preLaunchTask": "g++"
    }
  ]
}